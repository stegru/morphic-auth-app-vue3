{
  "name": "morphic-community",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "node_modules/.bin/eslint src/**/*.{js,vue} && npm run i18n:report",
    "build-dev": "vue-cli-service build --mode development --dest dev",
    "dev": "NODE_ENV=local vue-cli-service serve",
    "test": "true",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "updateEmailHtml": "node ./updateEmailHtml.js",
    "postinstall": "npm run updateEmailHtml"
  },
  "dependencies": {
    "@popperjs/core": "2.5.4",
    "arrow-line": "^0.7.4",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.15.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-easy-dnd": "^1.8.4",
    "vue-i18n": "^8.22.3",
    "vue-recaptcha-v3": "^1.9.0",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.1",
    "vuelidate": "^0.7.5",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@types/stripe-v3": "^3.1.25",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "^4.4.3",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^23.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^36.0.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "jest-mock-axios": "^4.3.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-i18n": "~2.0.1",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.6.11"
